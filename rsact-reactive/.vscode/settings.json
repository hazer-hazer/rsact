{
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.cargo.allTargets": false,
  "editor.formatOnSave": true,
  "rust-analyzer.check.features": ["single-thread", "debug-info"],
  "rust-analyzer.cargo.features": ["single-thread", "debug-info"],
  "rust-analyzer.testExplorer": true,
  "rustTestExplorer.rootCargoManifestFilePath": "${workspaceFolder}/Cargo.toml",
  "rust-analyzer.runnables.extraTestBinaryArgs": ["--test-threads=1"],
  "rustTestExplorer.logpanel": true,
  "cSpell.words": ["dirten", "rsact", "Uninit"]
}
